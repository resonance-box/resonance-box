{
  "name": "@resonance-box/store",
  "version": "0.0.11",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resonance-box/resonance-box.git"
  },
  "homepage": "https://github.com/resonance-box/resonance-box#readme",
  "bugs": {
    "url": "https://github.com/resonance-box/resonance-box/issues"
  },
  "license": "MIT",
  "author": "ot07",
  "engines": {
    "node": "20.x",
    "pnpm": ">=8.7.0"
  },
  "packageManager": "pnpm@8.7.0",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "_build": "tsc && vite build",
    "_clean": "rimraf dist",
    "_build:wasm": "wasm-pack build wasm-src --release --target web --out-dir ../src/generated/wasm --out-name store --no-pack",
    "_clean:wasm": "rimraf src/generated/wasm",
    "build": "pnpm _clean:wasm && pnpm _clean && pnpm _build:wasm && pnpm _build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.3"
  }
}
